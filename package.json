    "@types/react-addons-update": "^0.14.19",
    "@types/react-color": "^2.13.3",
    "@types/react-cropper": "^0.10.1",
    "@types/react-dom": "^16.0.7",
    "@types/react-dropzone": "^4.1.0",
    "@types/react-json-tree": "^0.6.6",
    "@types/react-onclickoutside": "^6.0.2",
