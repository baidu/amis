    "jest-canvas-mock": "^2.3.0",
    "js-yaml": "^3.10.0",
    "lint-staged": "^8.1.6",
    "marked": "^1.1.1",
    "mkdirp": "^1.0.4",
    "mobx-wiretap": "^0.12.0",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
